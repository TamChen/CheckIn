<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ssh.entity">
  <class name="com.csust.entity.Attend" table="attend" catalog="checkin">
    <id name="id" column="id" type="java.lang.Integer">
      <generator class="identity">
      </generator>
    </id>
    <property name="userno" column="userno" type="java.lang.Integer"></property>
    <property name="username" column="username" type="java.lang.String"></property>
    <property name="ip" column="ip" type="java.lang.String"></property>
    <property name="worktime" column="worktime" type="java.lang.Float"></property>
    <property name="freetime" column="freetime" type="java.lang.Float"></property>
    <property name="recordtime" column="recordtime" type="java.sql.Date"></property>
  </class>
</hibernate-mapping>
