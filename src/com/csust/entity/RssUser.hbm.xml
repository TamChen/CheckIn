<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.csust.entity">
  <class name="com.csust.entity.RssUser" table="rssuser" catalog="checkin">
    <id name="id" column="id" type="java.lang.Integer">
      <generator class="identity">
      </generator>
    </id>
    <property name="userno" column="userno" type="java.lang.Integer"></property>
    <property name="username" column="username" type="java.lang.String"></property>
      <property name="rssid" column="rssid" type="java.lang.Integer"></property>
   	  </class>
</hibernate-mapping>
